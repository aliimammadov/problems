#include <bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int n;
    long long x;
    cin >> n >> x;

    vector<long long> arr(n);
    for (int i = 0; i < n; i++) {
        cin >> arr[i];
    }
    vector<pair<long long, int>> v(n);
    for (int i = 0; i < n; i++) {
        v[i] = {arr[i], i};
    }
    sort(v.begin(), v.end());
    for (int i = 0; i < n; i++) {
        long long need = x - v[i].first;
        int left = i + 1, right = n - 1;

        while (left < right) {
            long long currentSum = v[left].first + v[right].first;
            if (currentSum == need) {
                cout << v[i].first << " "
                     << v[left].first << " "
                     << v[right].first << "\n";
                return 0;
            }
            else if (currentSum < need) {
                left++;
            }
            else {
                right--;
            }
        }
    }
    cout << -1 << "\n";
    return 0;
}
