#include <bits/stdc++.h>
using namespace std;

int main() {
    int n; 
    cin >> n;
    vector<int> v(n);
    for (int i = 0; i < n; ++i) {
        cin >> v[i];
    }
    unordered_set<int> s;
    int i = 0;
    int max_len = 0;
    for (int j = 0; j < n; ++j) {
        while (s.find(v[j]) != s.end()) {
            s.erase(v[i]); 
            i++; 
        }
        s.insert(v[j]); 
        max_len = max(max_len, j - i + 1); 
    }
    cout << max_len;
}
